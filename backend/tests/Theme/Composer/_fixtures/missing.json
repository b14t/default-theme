{
    "authors": [
        {
            "name": "Bjoern Lange",
            "email": "bjoern.lange@bestit-online.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "ButterflyEffect\\DefaultTheme\\": "backend/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ButterflyEffect\\DefaultTheme\\Tests\\": "backend/tests/"
        }
    },
    "description": "The default theme as a boilerplate for the butterfly effect.",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "butterfly-effect": {},
        "laravel": {
            "providers": [
                "ButterflyEffect\\DefaultTheme\\ButterflyEffectDefaultThemeServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "beta",
    "name": "b14t/default-theme",
    "require": {
        "php": ">=7.1",
        "bestit/butterfly-effect": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:best-it/butterfly-effect-package.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "bestit/php_codesniffer": "^0.1.0"
    }
}
